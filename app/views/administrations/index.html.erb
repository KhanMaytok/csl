<script type="text/javascript" >
function retornarLienzo(x) {
  var canvas = document.getElementById(x);
  if (canvas.getContext) {
    var lienzo = canvas.getContext("2d");   
    return lienzo;
  } else
    return false;
}

function dibujar() {
  var lienzo=retornarLienzo("lienzo1");
  if (lienzo) { 
    lienzo.beginPath();
    lienzo.strokeStyle="black";
    lienzo.lineWidth=1;
    lienzo.moveTo(550,5);
    lienzo.lineTo(550,412);
    lienzo.stroke();
 }
}
</script>


<body onLoad="dibujar()">
      <canvas id="lienzo1" width="800" height="600" style="position:absolute;">
