.row
	.col-md-12
		.row
			.col-md-4
			.col-md-4
				=form_tag services_path, method: 'get', remote: true do
					.row
						.col-md-6
							=text_field_tag :search, '', class: 'form-control'
						.col-md-6
							=submit_tag 'Buscar', class: 'btn btn-success'
			.col-md-4
				=button_tag 'Nuevo servicio', class: 'btn btn-success', data: { toggle: 'modal', target: '#serviceModal' }
				#serviceModal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "myModalLabel", :role => "dialog", :tabindex => "-1"}
					.modal-dialog
						.modal-content
							.list-group
								= render 'form'
				

#table
	=render 'table'